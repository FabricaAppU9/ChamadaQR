<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.fabricaapp.chamadaqr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/formulario_projetos"
            android:name=".activity.FormularioProjetosActivity" />
        <activity android:name=".activity.TelaCadastroActivity" />
      <activity
            android:name=".activity.TelaPrincipalActivity"
            android:label="@string/telaEventos" />
        <activity android:name=".activity.TelaQR" />
        <activity android:name=".activity.TelaDetalhesEvento">
        </activity>

    </application>

</manifest>